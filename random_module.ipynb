    "import random\n",
    "# randomモジュールのインポート\n",
    "answer = random.randint(1, 10)\n",
    "# randintでランダムにinteger取得\n",
    "\n",
    "print(\"Your guess? \",  end=\"\")\n",
    "guess = int(input())\n",
    "\n",
    "if answer == guess:\n",
    "    print(\"Bingo!\")\n",
    "elif answer > guess:\n",
    "    print(\"Bigger\")\n",
    "else:\n",
    "    print(\"Smaller\")\n",
    "\n",
    "print(\"[Answer:  %i]\" %answer)\n"
   ]
